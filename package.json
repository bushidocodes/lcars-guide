{
  "name": "borg-no-more",
  "version": "1.0.0",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true,
      "mocha": true
    },
    "extends": "airbnb",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "import/extensions": 0,
      "comma-dangle": 0,
      "react/prop-types": 0
    }
  },
  "description": "Borg No More is the ultimate platform for fans of Star Trek Season IV.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./public/bundle.js && rimraf ./public/bundle.js.map && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "rimraf ./public/bundle.js && rimraf ./public/bundle.js.map && cross-env NODE_ENV=development node dev-server.js",
    "test": "mocha --compilers js:babel-register app/test.js app/**/*.test.js app/**/*.test.jsx",
    "test-watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spmcbride1201/borg-no-more.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spmcbride1201/borg-no-more/issues"
  },
  "homepage": "https://github.com/spmcbride1201/borg-no-more#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-devtools-extension": "^2.13.0",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "http-server": "^0.9.0",
    "material-ui": "^0.17.0",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "reactable": "^0.14.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2"
  }
}
